sono <- function(txt){
aFile = readLines(txt)
myCorpus = Corpus(VectorSource(aFile))
myCorpus = tm_map(myCorpus, tolower)
myCorpus = tm_map(myCorpus, removePunctuation)
myCorpus = tm_map(myCorpus, removeNumbers)
myCorpus = tm_map(myCorpus, removeWords, stopwords(kind="portuguese"))
myDTM = TermDocumentMatrix(myCorpus, control = list(minWordLength = 1))
m = as.matrix(myDTM)
v = sort(rowSums(m), decreasing = TRUE)
v<-as.data.frame(v)
x<-data.frame(row.names(v),v[,1])
# wordcloud2(data = x, size = .5,
#            shape = "circle",
#            fontFamily = "CMU Sans Serif", rotateRatio = 0.5,
#            ellipticity = 0.9, color = "brown")
}
d<-readLines("www/data/wcloud/lula2003_limpo.txt")
sono(d)
d<-readLines("www/data/wcloud/lula2003_limpo.txt")
sono(d)
sono <- function(txt){
aFile = readLines(txt)
myCorpus = Corpus(VectorSource(aFile))
myCorpus = tm_map(myCorpus, tolower)
myCorpus = tm_map(myCorpus, removePunctuation)
myCorpus = tm_map(myCorpus, removeNumbers)
myCorpus = tm_map(myCorpus, removeWords, stopwords(kind="portuguese"))
myDTM = TermDocumentMatrix(myCorpus, control = list(minWordLength = 1))
m = as.matrix(myDTM)
v = sort(rowSums(m), decreasing = TRUE)
v<-as.data.frame(v)
x<-data.frame(row.names(v),v[,1])
# wordcloud2(data = x, size = .5,
#            shape = "circle",
#            fontFamily = "CMU Sans Serif", rotateRatio = 0.5,
#            ellipticity = 0.9, color = "brown")
return(x)
}
